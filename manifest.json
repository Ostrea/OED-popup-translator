{
    "manifest_version": 2,

    "name": "OED popup translator",
    "description": "This extension shows a word translation from OED of the currently selected word",
    "version": "1.0",
    
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Click here!"
    },
    "permissions": [
        "<all_urls>"
    ],
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}