{
    "manifest_version": 2,

    "name": "OED popup translator",
    "description": "This extension shows a word translation from OED of the currently selected wod",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Click here!"
   },
    "permissions": [
        "activeTab",
        "https://www.oxforddictionaries.com/"
    ]
}
